                "summary": "Ambil semua data mahasiswa",
                "operationId": "dbf3137c7d1805b9cf58fe27b70a6e89",
                "responses": {
                    "200": {
                        "description": "Berhasil mengambil data"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": ["Mahasiswa"],
                "summary": "Tambah data mahasiswa",
                "operationId": "66fe54445ec2d86b3347d43216a5045f",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nim",
                                    "nama_mahasiswa",
                                    "fakultas",
                                    "jurusan"
                                ],
                                "properties": {
                                    "nim": {
                                        "type": "string",
                                        "example": "G.211.22.0100"
                                    },
                                    "nama_mahasiswa": {
                                        "type": "string",
                                        "example": "Nizar Hidayatul Ikhsan"
                                    },
                                    "fakultas": {
                                        "type": "string",
                                        "example": "Teknik"
                                    },
                                    "jurusan": {
                                        "type": "string",
                                        "example": "Informatika"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Berhasil menambahkan data"
                    },
                    "400": {
                        "description": "Validasi gagal"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
